extern c_exit(retval: word);

struct Number { a: word, }

impl Number {
    @static
    fn make(n: word) -> @: @ { a: n, };

    fn times(n: word) -> @: @.make(.a * n);
    fn square -> @: .times(.a);
    fn get -> word: .a;
}

fn exit_number(n: Number): c_exit(n.get());

impl Number {
    fn exit: exit_number(.);
}

fn main {
    Number.make(3).times(2).square().exit();
}
