extern c_exit(retval: word);

struct Number { a: word, }

impl Number {
    @static
    fn make(n: word) -> @: @ { a = n, };

    fn times(n: word) -> @: @.make(self.a * n);
    fn square -> @: self.times(self.a);
    fn get -> word: self.a;
}

fn main {
    c_exit(Number.make(3).times(2).square().get());
}
